{
  "name": "fidipa-website-static",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build:css": "npx tailwindcss -i ./new_static_site/css/style.css -o ./new_static_site/css/style.css --content './new_static_site/**/*.html' './new_static_site/js/**/*.js'",
    "watch:css": "npx tailwindcss -i ./new_static_site/css/style.css -o ./new_static_site/css/style.css --content './new_static_site/**/*.html' './new_static_site/js/**/*.js' --watch",
    "lint": "eslint ./new_static_site/js/**/*.js"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  }
}